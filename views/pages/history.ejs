<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
    
  </head>
  
  <body class="container">
  
    <header>
      <%- include('../partials/header'); %>
    </header>
  <main>

    <div class="jumbotron">


      <h1 class="display-4">History</h1>

      <table class="table table-striped table-dark">
        <thead class="thead-light">
          <tr>
            <th>Nr</th>
            <th>Action name</th>
            <th>Target title</th>
            <th>Created at</th>
            <th>Target type</th>
          </tr>
        </thead>
        <% for(var i=0; i < viewData.length; i++) { %>
          <tr>
            <td>
              <%= i+1 %>
            </td>
            <td>
              <%= viewData[i].action_name %>
            </td>
            <td>
              <%= viewData[i].target_title %>
            </td>
            <td>
              <%= viewData[i].created_at %>
            </td>
            <td>
              <%= viewData[i].target_type %>
            </td>
          </tr>
          <% } %>
      </table>
  </main>

  <footer>
    <%- include('../partials/footer'); %>
  </footer>

</body>


</html>